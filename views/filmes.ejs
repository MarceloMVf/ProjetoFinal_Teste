<!DOCTYPE html>
<html lang="pt-br">
<head id="teste">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css" />
    <title>Meu catalogo</title>
</head>
<body class="">
  <section class="wrapper">
    <div id="stars"></div>
    <div id="stars2"></div>
    <div id="stars3"></div>
    <div id="title"></div>

    <header class="header">
      <div class="center">
        <div class="menu" id="menuId" > 
          <ul id="navegacao">
            <li><a  href="/"><strong">INICIO</strong></li>
            <li><a href="filmes">FILMES</li>
            <li><a href="series">SERIES</li>
            <li><a href="livros">LIVROS</li>
            <li><a href="carros">CARROS</li>
            <li><a href="videos">VIDEOS</li>
          </ul>
        </div>
    </header>

<section class="container">
  <% filmes.forEach((item, id) => { %>
  <ul>
    <div>
      <a href="/detalhes/detalhesFilmes/<%=id%>">
        <img src="<%= item.imagem %>" width="200" alt="<%= item.nome %>" />   
        <p><h2 class="nameItem"> <%= item.titulo %></h2></p>     
  </ul>
  <% }) %>
</section>
</section>

<script src="/js/script.js"></script>
</body>
</html>