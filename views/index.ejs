<!DOCTYPE html>
<html lang="pt-br">
<head id="teste">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css" />
    <title>Meu catalogo</title>
</head>

<!--Lista: Uma tela responsável por mostrar a lista de todos os itens do catálogo.-->
<body>
  <header class="header">
    <div class="center">
      <div class="menu">
        <ul>
          <li><a href="/">INICIO</li>
          <li><a href="filmes">FILMES</li>
          <li><a href="series">SERIES</li>
          <li><a href="livros">LIVROS</li>
          <li><a href="carros">CARROS</li>
          <li><a href="videos">VIDEOS</li>
        </ul>
      </div>
    </div>
  </header>
  <nav class="">
<a href="/filmes">FILMES</a>
<a href="/series">SERIES</a>
<a href="/livros">LIVROS</a>
<a href="/carros">CARROS</a>
<a href="/videos">VIDEOS</a>
  </nav>
<section>FILMES</section>
<section class="">
  <% filmes.forEach((item, id) => { %>
  <ul>
    <div>
      <a href="/detalhesFilmes/<%=id%>">
        <img src="<%= item.imagem %>" width="200" alt="<%= item.nome %>" />        
  </ul>
  <% }) %>
</section>
<section>SERIES</section>
<section class="">
  <% series.forEach((item, id) => { %>
  <ul>
    <div>
      <a href="/detalhesSeries/<%=id%>">
        <img src="<%= item.imagem %>" width="200" alt="<%= item.nome %>" />        
  </ul>
  <% }) %>
</section>
<section>LIVROS</section>
<section class="">
  <% livros.forEach((item, id) => { %>
  <ul>
    <div>
      <a href="/detalhesLivros/<%=id%>">
        <img src="<%= item.imagem %>" width="200" alt="<%= item.nome %>" />        
  </ul>
  <% }) %>
</section>
<section>CARROS</section>
<section class="">
  <% carros.forEach((item, id) => { %>
  <ul>
    <div>
      <a href="/detalhesCarros/<%=id%>">
        <img src="<%= item.imagem %>" width="200" alt="<%= item.nome %>" />        
  </ul>
  <% }) %>
</section>
<section>VIDEOS</section>
<section class="">
  <% videos.forEach((item, id) => { %>
  <ul>
    <div>
      <a href="/detalhesVideos/<%=id%>">
        <img src="<%= item.imagem %>" width="200" alt="<%= item.nome %>" />        
  </ul>
  <% }) %>
</section>
<body>
    <script src="/js/script.js"></script>
</body>
</html>